#tmux alias
alias t="tmux"
alias ta="tmux attach -t"
alias tv="tmux split-window -v"
alias th="tmux split-window -h"
alias tl="tmux ls"


#github alias
alias g="git"
alias gd="git diff"
alias gs="git status"
alias gl="git log"
gsm() {
  git show $1; git diff "$1^" "$1"
}
gsmv() {
  git difftool "$1^" "$1"
}

alias v="vim"
alias r="realpath"

umask 0002

alias f="find"
